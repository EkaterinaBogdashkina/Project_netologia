    "# Задание 1\n",
    "import json\n",
    "f = open('purchase_log.txt\\\\purchase_log.txt', encoding='utf-8')\n",
    "i=0\n",
    "next(f)\n",
    "for line in f:\n",
    "    line = json.loads(line)\n",
    "    i+=1\n",
    "    print(line[\"user_id\"],'\\'', line[\"category\"],'\\'')\n",
    "    if i>1:\n",
    "       break\n",
    "f.close()   \n"
   ]
  }
